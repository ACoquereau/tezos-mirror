parameter (map (int :k) (int :e));
storage (pair (int :k) (int :e));
code { CAR; PUSH @acc_e (int :e) 0; PUSH @acc_k (int :k) 0; PAIR; SWAP;
       ITER $my_key $my_elt
            { DIP {DUP; CAR; DIP{CDR}}; DUP; # Last instr
              DIP{CAR; ADD}; SWAP; DIP{CDR; ADD}; PAIR };
       NIL operation; PAIR}
