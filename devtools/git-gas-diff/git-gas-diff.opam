opam-version: "2.0"
depends: [
  "dune" {>= "2.9"}
  "num" {>= "1.4"}
  "re" {>= "10.1.3"}
]
build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "--promote-install-files=false"
    "@install"
  ]
  ["dune" "install" "-p" name "--create-install-files" name]
]
